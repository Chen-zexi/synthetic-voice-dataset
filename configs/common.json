{
  "followup_turns": {
    "num_turns_lower_limit": 2,
    "num_turns_upper_limit": 4,
    "sample_limit": 25,
    "victim_awareness_levels": ["not", "not", "not", "not", "not", "tiny", "very"]
  },
  "preprocessing": {
    "input_file": "scamGen_combined_first_20k.txt",
    "mapped_suffix": "_mapped.txt"
  },
  "translation": {
    "service": "qwen",
    "max_lines": 1000,
    "english_output": "scamGen_combined_first_20k_en.txt",
    "chinese_code": "zh-CN",
    "qwen_model": "qwen-mt-turbo",
    "qwen_base_url": "https://dashscope-intl.aliyuncs.com/compatible-mode/v1",
    "max_concurrent_translations": 20
  },
  "multi_turn": {
    "english_output": "scam_conversation_english.json",
    "max_conversation": 25
  },
  "legit_call": {
    "num_conversations": 25
  },
  "voice_generation": {
    "sample_limit": 25,
    "model_id": "eleven_multilingual_v2",
    "model_v3_enabled": false,
    "output_format": "mp3_44100_128",
    "voice_speed": 1.08,
    "silence_duration_ms": 500,
    "background_volume_reduction_db": 18,
    "bandpass_filter": {
      "low_freq": 300,
      "high_freq": 3400
    },
    "voice_settings": {
      "stability": 0.5,
      "similarity_boost": 0.5,
      "style": 0.0,
      "speaker_boost": true
    },
    "quality_settings": {
      "use_high_quality": false,
      "high_quality_format": "pcm_44100",
      "optimize_streaming_latency": 0
    },
    "v3_features": {
      "use_audio_tags": false,
      "emotional_context": true,
      "conversation_context": true,
      "default_emotion_scam": "urgent",
      "default_emotion_legit": "friendly"
    }
  },
  "post_processing": {
    "scam_label": 1,
    "legit_label": 0,
    "audio_zip_names": {
      "scam": "scam_conversation_audio.zip",
      "legit": "legit_conversation_audio.zip"
    }
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4.1-mini",
    "max_concurrent_requests": 10,
    "temperature": 1.0,
    "max_tokens": null,
    "top_p": 0.95,
    "n": 1
  },
  "translation_cache": {
    "enabled": true,
    "use_cache": true,
    "cache_dir": "data/translation_cache",
    "cache_service": "qwen",
    "force_refresh": false
  }
}