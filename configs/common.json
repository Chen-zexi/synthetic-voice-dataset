{
  "followup_turns": {
    "num_turns_lower_limit": 2,
    "num_turns_upper_limit": 4,
    "sample_limit": 25,
    "victim_awareness_levels": [
      "not",
      "not",
      "not",
      "not",
      "not",
      "tiny",
      "very"
    ]
  },
  "preprocessing": {
    "input_file": "scamGen_combined_first_20k.txt",
    "mapped_suffix": "_mapped.txt"
  },
  "translation": {
    "service": "qwen",
    "max_lines": 1000,
    "english_output": "scamGen_combined_first_20k_en.txt",
    "chinese_code": "zh-CN",
    "qwen_model": "qwen-mt-turbo",
    "qwen_base_url": "https://dashscope-intl.aliyuncs.com/compatible-mode/v1",
    "max_concurrent_translations": 20
  },
  "multi_turn": {
    "english_output": "scam_conversation_english.json",
    "max_conversation": 25
  },
  "legit_call": {
    "num_conversations": 25
  },
  "voice_generation": {
    "sample_limit": 25,
    "model_id": "eleven_turbo_v2_5",
    "output_format": "mp3_44100_128",
    "voice_speed": 1.08,
    "silence_duration_ms": 500,
    "background_volume_reduction_db": 18,
    "bandpass_filter": {
      "low_freq": 300,
      "high_freq": 3400
    },
    "audio_effects": {
      "enable_background_noise": true,
      "enable_call_end_effect": true,
      "enable_bandpass_filter": true,
      "background_noise_level": 1.0,
      "call_end_volume": 1.0
    }
  },
  "post_processing": {
    "scam_label": 1,
    "legit_label": 0,
    "audio_zip_names": {
      "scam": "scam_conversation_audio.zip",
      "legit": "legit_conversation_audio.zip"
    },
    "exclude_fields": [
      "first_turn",
      "voice_mapping",
      "scenario",
      "metadata",
      "victim_awareness",
      "category"
    ]
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4.1-mini",
    "max_concurrent_requests": 10,
    "temperature": 1.0,
    "max_tokens": null,
    "top_p": 0.95,
    "n": 1
  },
  "translation_cache": {
    "enabled": true,
    "use_cache": true,
    "cache_dir": "data/translation_cache",
    "cache_service": "qwen",
    "force_refresh": false
  }
}